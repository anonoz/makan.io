.row
  table.small-12
    thead
      th.small-1 ID
      th.small-9 Title
      th.small-2
    tbody 
      - city.subvendors.each do |subvendor|
        tr
          td = subvendor.id
          td = subvendor.title
          td
      = form_for(@new_subvendor, url: {action: "create"},
      	         remote: false) do |f|
        = f.hidden_field :place_city_id, value: city.id
      	tr
      	  td New
      	  td = f.text_field :title, placeholder: "Ah Boy Nasi Lemak", required: true
      	  td = f.submit "Create", class: "button tiny expand success"